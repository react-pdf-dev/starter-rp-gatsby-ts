{
  "setupTasks": [
    {
      "name": "Installing Dependencies",
      "command": "npm install"
    }
  ],

  "tasks": {
    "develop": {
      "name": "develop",
      "command": "npm run develop"
    },
    "start": {
      "name": "start",
      "command": "npm run start",
      "runAtStart": true,
      "preview": {
        "port": 8000
      }
    },
    "build": {
      "name": "build",
      "command": "npm run build"
    },
    "serve": {
      "name": "serve",
      "command": "npm run serve"
    },
    "clean": {
      "name": "clean",
      "command": "npm run clean"
    },
    "typecheck": {
      "name": "typecheck",
      "command": "npm run typecheck"
    }
  }
}
